#define FILEVER                         0,0,4,40
#define FILEVERSTR                      "0.0.4.40\0"
#define PRODUCTVER                      FILEVER
#define PRODUCTVERSTR                   FILEVERSTR

#include "afxres.h"
#include "shresdef.h"

LANGUAGE LANG_CHINESE, SUBLANG_CHINESE_SIMPLIFIED

VS_VERSION_INFO VERSIONINFO
